{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "preview": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "build"
      ]
    },
    "lint": {},
    "test": {
      "dependsOn": [
        "^test"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
